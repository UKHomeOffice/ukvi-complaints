{
  "license": "MIT",
  "config": {
    "reporter": "--reporter mocha-multi-reporters --reporter-options configFile=test/reporter-config"
  },
  "scripts": {
    "start": "node app.js",
    "start:dev": "hof-build watch --env",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint . --config ./node_modules/eslint-config-hof/default.js",
    "test:unit": "nyc --report-dir=test/coverage --reporter=lcov mocha \"test/unit/**/*.spec.js\" --timeout 10000 --require test/test-helpers.js -- $npm_package_config_reporter/unit.json",
    "test:check": "mocha \"test/unit/**/*.spec.js\"",
    "sqs-setup": "docker build -q -t sqs-mock test/integration/sqs-mock && docker run --name sqs-mock -p 9324:9324 -d sqs-mock",
    "sqs-cleanup": "docker stop sqs-mock && docker container rm sqs-mock",
    "build": "hof-build",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "govuk-frontend": "^3.5.0",
    "hof": "^18.2.4",
    "hof-behaviour-emailer": "^2.2.1",
    "hof-behaviour-summary-page": "^3.3.0",
    "hof-build": "^4.0.0",
    "hof-component-date": "^1.4.0",
    "hof-middleware": "^2.2.4",
    "hof-theme-govuk": "^5.2.4",
    "hof-util-countries": "^1.0.0",
    "jquery": "^3.6.0",
    "jsonschema": "^1.4.0",
    "moment": "^2.29.1",
    "sqs-producer": "^2.1.0",
    "typeahead-aria": "^1.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@danmasta/mocha-sonar": "^1.0.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.30.0",
    "eslint-config-hof": "^1.0.4",
    "mocha": "^9.0.3",
    "mocha-multi-reporters": "^1.5.1",
    "mochawesome": "^6.2.2",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^11.1.1",
    "sinon-chai": "^3.7.0"
  },
  "name": "ukvi-complaints",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "description": "",
  "engines": {
    "node": "14.16.1"
  }
