{
  "license": "MIT",
  "config": {
    "reporter": "--reporter mocha-multi-reporters --reporter-options configFile=test/reporter-config"
  },
  "scripts": {
    "start": "node app.js",
    "start:dev": "hof-build watch --env",
    "start:mock": "node app.js silent mock-sqs",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint .",
    "test:unit": "nyc --report-dir=test/coverage --reporter=lcov mocha \"test/unit/**/*.spec.js\" --require test/test-helpers.js -- $npm_package_config_reporter/unit.json",
    "test:check": "mocha \"test/unit/**/*.spec.js\"",
    "test:integration": "mocha \"test/integration/**/*.spec.js\" --require test/test-helpers.js test/integration/integration-test-helpers.js --timeout 10000 --browser-engine=chromium",
    "test:integration:server": "start-server-and-test start:mock http://localhost:8080/reason?automation-test test:integration",
    "test:integration:sqs": "npm run sqs-setup && npm run test:sqs:server && npm run sqs-cleanup",
    "sqs-setup": "docker build -q -t sqs-mock test/integration/sqs-mock && docker run --name sqs-mock -p 9324:9324 -d sqs-mock",
    "sqs-cleanup": "docker stop sqs-mock && docker container rm sqs-mock",
    "build": "hof-build",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "govuk-frontend": "^3.5.0",
    "hof": "^13",
    "hof-behaviour-emailer": "^2.2.0",
    "hof-behaviour-summary-page": "^2.0.0",
    "hof-build": "^1.3.4",
    "hof-component-date": "^1.4.0",
    "hof-template-mixins": "^5.0.1",
    "hof-theme-govuk": "^4.3.0",
    "hof-util-countries": "^1.0.0",
    "jquery": "^3.6.0",
    "jsonschema": "^1.4.0",
    "moment": "^2.29.1",
    "sqs-producer": "^2.1.0",
    "typeahead-aria": "^1.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@danmasta/mocha-sonar": "^1.0.0",
    "aws-sdk": "^2.901.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.16.0",
    "eslint-config-homeoffice": "^2.1.0",
    "mocha": "^8.3.2",
    "mocha-multi-reporters": "^1.5.1",
    "mochawesome": "^6.2.2",
    "nyc": "^15.1.0",
    "playwright": "^1.10.0",
    "proxyquire": "^2.1.3",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.6.0",
    "start-server-and-test": "^1.12.1"
  },
  "name": "ukvi-complaints",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "description": "",
  "engines": {
    "node": "14.16.1"
  }
}
